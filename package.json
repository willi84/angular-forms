{
  "name": "angular-forms",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "install": "npm install yarn@1.3.2",
    "start": "ng serve",
    "start:host": "ng serve --port=4200 -o --host=${HOSTNAME}   --proxy-config proxy.conf.json -e=dev",
    "start:host:prod": "ng serve --port=4200 -o --host=${HOSTNAME}   --proxy-config proxy.conf.json -e=prod",
    "build": "./node_modules/@angular/cli/bin/ng build",
    "build:prod": "./node_modules/@angular/cli/bin/ng build --prod --build-optimizer -sm -vc=false && npm run concat",
    "build:dist": "./node_modules/@angular/cli/bin/ng build --prod --build-optimizer  && npm run concat:dist",
    "concat": "./node_modules/gulp/bin/gulp.js concat",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "test": "./node_modules/@angular/cli/bin/ng test",
    "test:prod": "./node_modules/@angular/cli/bin/ng test -cc -sm -e=dev --single-run",
    "test:test": "./node_modules/@angular/cli/bin/ng test -cc -sm -e=dev",
    "lint": "./node_modules/@angular/cli/bin/ng lint",
    "single": "./node_modules/.bin/protractor conf/single.conf.js",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "yarn": "^1.3.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@compodoc/compodoc": "^1.0.9",
    "@types/jasmine": "~2.8.3",
    "@types/jasmine-matchers": "^0.2.30",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
